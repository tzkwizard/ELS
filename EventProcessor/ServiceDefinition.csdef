<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="EventProcessor" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="ReceiverRole" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="Microsoft.ServiceBus.ConnectionString" />
      <Setting name="AzureStorageConnectionString" />
      <Setting name="eventHubName" />
      <Setting name="consumerGroupName" />
      <Setting name="numberOfPartitions" />
      <Setting name="DocumentDBUrl" />
      <Setting name="DocumentDBAuthorizationKey" />
      <Setting name="Database" />
      <Setting name="Collection" />
    </ConfigurationSettings>
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <Certificates>
    </Certificates>
  </WorkerRole>
</ServiceDefinition>